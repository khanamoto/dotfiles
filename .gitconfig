# This is Git's per-user configuration file.
[user]
	name = Kenji Hanamoto
	email = khanamoto10@gmail.com
[color]
	ui = auto
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
